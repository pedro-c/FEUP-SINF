@{
    ViewBag.Title = "_Header";
}

<div class="container body">
    <div class="main_container">
        <div class="col-md-3 left_col">
            <div class="left_col scroll-view">
                <div class="navbar nav_title" style="border: 0;">
                    <a href="index.html" class="site_title"><span>@Model.companyName</span></a>
                </div>
                <div class="clearfix"></div>
                <!-- menu profile quick info -->
                <div class="profile clearfix">
                    <div class="profile_info">
                        <span>@Model.street</span>
                        <span>@Model.city</span>
                        <span>@Model.country</span>
                    </div>
                </div>
                <!-- /menu profile quick info -->
                <br />
                <!-- sidebar menu -->
                <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                    <div class="menu_section">
                        <h3>General</h3>
                        <ul class="nav side-menu">
                            <li>
                                <a href="/Financial"><i class="fa fa-euro"></i> Financial</a>
                            </li>
                            <li>
                                <a href="/Management"><i class="fa fa-edit"></i> Management</a>
                            </li>
                            <li>
                                <a href="/Sales"><i class="fa fa-bar-chart-o"></i> Sales</a>
                            </li>
                            <li>
                                <a href="/Inventory"><i class="fa fa-shopping-cart"></i> Inventory</a>
                            </li>
                            <li>
                                <a href="/Data"><i class="fa fa-edit"></i> Load SAF-T</span></a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <!-- top navigation -->
        <div class="top_nav">
            <div class="nav_menu">
                <nav>
                    <div class="nav toggle">
                        <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                    </div>

                    <ul class="nav navbar-nav navbar-right">
                        <li class="">
                            <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                <img src="images/img.jpg" alt="">Saft Date from @Model.startDate.ToString("MM/dd/yyyy") to @Model.endDate.ToString("MM/dd/yyyy")
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>
